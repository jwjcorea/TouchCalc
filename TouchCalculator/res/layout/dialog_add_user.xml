<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent" android:minWidth="200dp" android:background="@color/darkgrey">
	<Button android:layout_height="wrap_content" android:id="@+id/btnTel"
		android:text="전화번호부" android:layout_width="match_parent"
		android:textSize="20dp" android:layout_marginLeft="10dp"
		android:layout_marginRight="10dp" android:layout_marginTop="10dp"></Button>
	<Button android:layout_height="wrap_content" android:id="@+id/btnDirect"
		android:text="직접 입력" android:layout_width="match_parent"
		android:textSize="20dp" android:layout_marginLeft="10dp"
		android:layout_marginRight="10dp" android:layout_marginBottom="10dp"></Button>

</LinearLayout>
